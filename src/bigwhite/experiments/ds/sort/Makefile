all: 
	gcc -g -o bubble_sort bubble_sort.c ../utils/utils.c -I ../utils
	gcc -g -o insert_sort insert_sort.c ../utils/utils.c -I ../utils
	gcc -g -o select_sort select_sort.c ../utils/utils.c -I ../utils
	gcc -g -o quick_sort quick_sort.c ../utils/utils.c -I ../utils

clean:
	rm -f bubble_sort insert_sort select_sort quick_sort
